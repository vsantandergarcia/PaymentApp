<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--Brandon Grotesque Bold-->
    <style name="TextBrandonGrotesqueBold">
        <item name="android:fontFamily">@font/brandongrotesque_bold</item>
    </style>

    <style name="TextBrandonGrotesqueBold14" parent="TextBrandonGrotesqueBold">
        <item name="android:textSize">@dimen/text_size_14</item>
    </style>

    <style name="TextBrandonGrotesqueBold14.Black" parent="TextBrandonGrotesqueBold14">
        <item name="android:textColor">@android:color/black</item>
